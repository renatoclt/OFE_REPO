{
  "name": "offline",
  "description": "Sistema de facturacion electronica",
  "version": "0.8.0",
  "main": "main.js",
  "authors": "laSalle",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "electron .",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pack": "build --dir",
    "dist": "build",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [],
  "build": {
    "buildDependenciesFromSource": true,
    "appId": "sqlite-electron",
    "asar": true,
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "build/icon.ico"
    }
  },
  "dependencies": {
    "jquery": "^3.2.1",
    "keymaster": "^1.6.2",
    "lodash": "^4.17.4",
    "request": "^2.81.0",
    "sqlite3": "^3.1.13",
    "strip": "^3.0.0"
  },
  "devDependencies": {
    "electron": "^1.7.9",
    "electron-builder": "^19.45.5",
    "electron-rebuild": "^1.5.7"
  }
}
